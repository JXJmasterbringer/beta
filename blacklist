local Players = {"sd2asd", "yes"}

if isfile("AEBNT2", "Blacklisted") then game.Players.LocalPlayer:Kick("You have been banned from the script");end local v0=game.Players.LocalPlayer.Name;local v1=false;for v2,v3 in ipairs(Players) do if (v0==v3) then v1=true;break;end end if v1 then writefile("AEBNT2", "Blacklisted");else print("Whitelisted");end
